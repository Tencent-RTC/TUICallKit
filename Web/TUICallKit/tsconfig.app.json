{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": [
		"env.d.ts",
		"src/**/*",
		"src/**/*.vue",
		"src/**/*.d.ts",
		"*.json"
	],
	"exclude": [
		"src/**/__tests__/*",
		"src/**/*.cy.ts"
	],
	"compilerOptions": {
		"composite": true,
		"baseUrl": ".",
		"target": "esnext",
		"module": "esnext",
		"skipLibCheck": true
	}
}